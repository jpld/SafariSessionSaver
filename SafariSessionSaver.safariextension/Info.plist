<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Author</key>
	<string>jpld</string>
	<key>CFBundleDisplayName</key>
	<string>SafariSessionSaver</string>
	<key>CFBundleIdentifier</key>
	<string>com.github.jpld.safarisessionsaver</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleShortVersionString</key>
	<string>0.0.0</string>
	<key>CFBundleVersion</key>
	<string>0.0.0</string>
	<key>Chrome</key>
	<dict>
		<key>Global Page</key>
		<string>global.html</string>
		<key>Toolbar Items</key>
		<array>
			<dict>
				<key>Command</key>
				<string>storeSession</string>
				<key>Identifier</key>
				<string>store</string>
				<key>Image</key>
				<string>toobar_button.png</string>
				<key>Include By Default</key>
				<true/>
				<key>Label</key>
				<string>Store the current browsing session</string>
				<key>Palette Label</key>
				<string>Store Session</string>
			</dict>
			<dict>
				<key>Command</key>
				<string>recallSession</string>
				<key>Identifier</key>
				<string>recall</string>
				<key>Image</key>
				<string>toobar_button.png</string>
				<key>Label</key>
				<string>Recall the stored browsing session</string>
				<key>Palette Label</key>
				<string>Recall Session</string>
			</dict>
		</array>
	</dict>
	<key>Content</key>
	<dict>
		<key>Scripts</key>
		<dict/>
	</dict>
	<key>Description</key>
	<string>Store and recall browsing sessions</string>
	<key>ExtensionInfoDictionaryVersion</key>
	<string>1.0</string>
	<key>Permissions</key>
	<dict>
		<key>Website Access</key>
		<dict>
			<key>Include Secure Pages</key>
			<true/>
			<key>Level</key>
			<string>All</string>
		</dict>
	</dict>
	<key>Website</key>
	<string>http://github.com/jpld/SafariSessionSaver</string>
</dict>
</plist>
